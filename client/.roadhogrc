{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    },
    "proxy": {
      "/api": {
        "target": "localhost:8000",
        "changeOrigin": true,
        "pathRewrite": { "^/api" : "/api" }
      }
    }
  }
}
